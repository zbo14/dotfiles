#!/bin/bash

alias al="alias"

al ..="cd .."
al -- -="cd -"
al clr="clear"
al dsk="cd ~/Desktop"
al dwn="cd ~/Downloads"
al doc="cd ~/Documents"
al gr="grep"

## git
al g="git"
al ga="g add ."
al gc="g commit -m"
al gcl="g clone"
al gl="g log"
al go="g remote add origin"
al gp="g push origin"
al gpl="g pull origin"
al gs="g status"

al ifc="ifconfig"
al l="ls -l"
al la="l -a"
al ns="netstat"

## npm
al n="npm"
al ni="n i"
al nl="n login"
al np="n publish"
al nr="n r"
al nu="n unpublish"
al nup="n update"

al pic="cd ~/Pictures"
al prj="cd ~/Projects"
al py="python"
al py3="python3"
al rr="rm -rf"
al sudo="sudo " # allow aliases to be sudo'ed
al trc="traceroute"
